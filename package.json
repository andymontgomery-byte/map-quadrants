{
  "name": "map-quadrants",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:data": "node scripts/build-data.js",
    "build:norms": "node scripts/convert-norms.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "playwright test",
    "test:smoke": "playwright test smoke",
    "test:ui": "playwright test --ui",
    "deploy": "npm run build && cp -r public/data dist/ && gh-pages -d dist",
    "compare": "node scripts/compare-screenshots.js",
    "compare:nwea": "node scripts/nwea-vision-compare.js",
    "record:nwea": "npx playwright codegen --save-storage=nwea-auth.json --output=scripts/nwea-navigation.spec.js https://teach.mapnwea.org",
    "auth:nwea": "node scripts/save-nwea-auth.js",
    "capture:nwea": "npx playwright test nwea-navigation --project=chromium --headed",
    "compare:full": "npx playwright test full-comparison --project=chromium --headed"
  },
  "dependencies": {
    "csv-parse": "^6.1.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.58.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "vite": "^6.0.5"
  }
}
